# URL base de la API del backend
VITE_API_BASE_URL=http://localhost:3000/api

# Nombre de la aplicación
VITE_APP_NAME=Sistema de Muebles

# Descripción de la aplicación
VITE_APP_DESCRIPTION=Sistema de venta de muebles con recomendaciones de IA

# Versión de la aplicación
VITE_APP_VERSION=1.0.0

# URL del sitio web
VITE_APP_URL=http://localhost:8080

# Configuración de desarrollo
VITE_DEV_MODE=true

# Configuración de logs en desarrollo
VITE_LOG_LEVEL=debug

# Configuración de cache
VITE_CACHE_ENABLED=true
VITE_CACHE_DURATION=300000

# Configuración de paginación
VITE_DEFAULT_PAGE_SIZE=12
VITE_MAX_PAGE_SIZE=50

# Configuración de archivos
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/gif,image/webp

# Configuración de notificaciones
VITE_TOAST_DURATION=5000
VITE_TOAST_POSITION=top-right

# Configuración de la IA (para mostrar en UI)
VITE_AI_ENABLED=true
VITE_AI_MAX_RECOMMENDATIONS=10

# URLs de redes sociales (para footer)
VITE_SOCIAL_FACEBOOK=https://facebook.com/muebles
VITE_SOCIAL_INSTAGRAM=https://instagram.com/muebles
VITE_SOCIAL_TWITTER=https://twitter.com/muebles
VITE_SOCIAL_LINKEDIN=https://linkedin.com/company/muebles

# Información de contacto
VITE_CONTACT_EMAIL=contacto@muebles.com
VITE_CONTACT_PHONE=+1234567890
VITE_CONTACT_ADDRESS=Calle Principal 123, Ciudad, País

# Google Analytics (opcional)
VITE_GA_TRACKING_ID=

# Configuración de PWA (opcional)
VITE_PWA_ENABLED=false